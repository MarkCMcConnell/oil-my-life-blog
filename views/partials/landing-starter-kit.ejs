<span class="anchor-id__span" id="starter-kit"></span>
<section class="starter-kit-section generic-section">
  <h2 class="generic-section__title">Starter Kit Essential Oils</h2>
  <div class="carousel">
    <i class="fa fa-chevron-left fa-4x carousel__controls" role="button"></i>
    <span class="sr-only">Previous</span>
    <div class="carousel__card">
      <h1 class="carousel__item-title"><%= oils[0].name %></h1>
      <div class="carousel__items">
        <div>
          <img class="carousel__image" src="<%= oils[0].image %>" alt="<%= oils[0].imageAlt %>" />
        </div>
        <div>
          <ul class="carousel__list">
            <% for(var i = 0; i < oils[0].information.length; i++) { %>
              <li class="carousel__list-item"><%= oils[0].information[i] %></li>
            <% } %>
          </ul>
        </div>
      </div>
    </div>
    <i class="fa fa-chevron-right fa-4x carousel__controls" role="button"></i>
    <span class="sr-only">Next</span>
  </div>
</section>
